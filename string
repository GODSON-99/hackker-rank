// link:https://www.hackerrank.com/challenges/java-strings-introduction/problem?isFullScreen=true
//code:
import java.io.*;
import java.util.*;
  

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String B=sc.next();
        /* Enter your code here. Print output to STDOUT. */
        int a= A.length();
        int b= B.length();
        int sum= a+b;
       
        System.out.println(sum);
        int c = A.compareTo(B);
        if(c>0)
            System.out.println("Yes");
        else if(c<=0)
            System.out.println("No");
        
        System.out.print(A.substring(0,1).toUpperCase()+ A.substring(1)+" ");
        System.out.print(B.substring(0,1).toUpperCase()+ B.substring(1));
        
       // System.out.println(A.substring(0,1));
        
    }
}



